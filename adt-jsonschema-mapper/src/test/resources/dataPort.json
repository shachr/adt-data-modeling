{
  "type": "object",
  "title": "DataPort",
  "description": "The data port associated with the data product, facilitating controlled data exchange.",
  "properties": {
    "id": {
      "type": "string",
      "description": "IRN"
    },
    "accessControlList": {
      "type": "array",
      "description": "The access control list that governs data port access and consumption.",
      "properties": {
        "credential": {
          "$ref": "#/definitions/Credential"
        },
        "entries": {
          "type": "array",
          "items": {
            "$ref": "#/definitions/AccessControlEntry"
          }
        }
      }
    }
  },
  "required": [
    "id",
    "accessControlList"
  ],

  "definitions": {
    "AccessControlEntry": {
      "type": "object",
      "properties": {
        "privileges": {
          "type": "array",
          "items": {
            "type": "string",
            "enum": ["w", "r", "-w", "-r"]
          }
        }
      },
      "required": ["privileges"]
    },
    "Credential": {
      "type": "object",
      "properties": {
        "type": {
          "type": "string",
          "enum": ["awsRole"]
        },
        "uri": {
          "type": "string"
        }
      },
      "required": ["type", "uri"]
    }
  }
}